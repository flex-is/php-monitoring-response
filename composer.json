{
    "name": "flexis/php-monitoring-response",
    "description": "Provides common http response structure for generic monitoring purposes",
    "keywords": ["monitoring"],
    "homepage": "https://github.com/flex-is/php-monitoring-response",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Miskovic",
            "email": "miskovic.martin@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "classmap-authoritative": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "flexis/php-cs-fixer-config": "^0.1",
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/php-code-coverage": "^10.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Flexis\\MonitoringResponse\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}